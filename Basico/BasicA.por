programa {
	// Faça um algoritmo que receba três números, calcule e mostre a multiplicação desses números.
  funcao inicio() {
    real x, y, z, multiplicacao
      escreva("Digite o 1° número: ")
      leia(x)
      escreva("Digite o 2° número: ")
      leia(y)
      escreva("Digite o 3° número: ")
      leia(z)
      limpa()
      multiplicacao= x*y*z
      escreva("O resultado da multiplicação é: ", multiplicacao)
  }
}

